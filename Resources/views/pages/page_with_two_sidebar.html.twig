<!-- ================== BEGIN page-js ================== -->
<script>
	App.setPageTitle('Color Admin | Page with Two Sidebar');
	App.restartGlobalFunction();
	App.setPageOption({
		appSidebarTwo: true,
		clearOptionOnLeave: true
	})

	$.when(
		$.getScript('{{ asset("plugins/@highlightjs/cdn-assets/highlight.min.js", uxadmin) }}'),
		$.Deferred(function( deferred ){
			$(deferred.resolve);
		})
	).done(function() {
		$.getScript('{{ asset("js/demo/render.highlight.js", uxadmin) }}'),
		$.Deferred(function( deferred ){
			$(deferred.resolve);
		})
	});
	
	$.ajax({
		url: 'partial/sidebar-end.html',
		success: function(html) {
			$('[data-id="app-extra-elm"]').html(html);
			
			App.restartGlobalFunction();
			
			$.when(
				$.getScript('{{ asset("plugins/jquery-sparkline/jquery.sparkline.min.js", uxadmin) }}'),
				$.getScript('{{ asset("plugins/jquery-knob/dist/jquery.knob.min.js", uxadmin) }}'),
				$.Deferred(function( deferred ){
					$(deferred.resolve);
				})
			).done(function() {
				$.getScript('{{ asset("js/demo/page-with-two-sidebar.demo.js", uxadmin) }}'),
				$.Deferred(function( deferred ){
					$(deferred.resolve);
				})
			});
		}
	});
</script>
<!-- ================== END page-js ================== -->
